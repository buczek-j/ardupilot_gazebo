<?xml version="1.0" ?>
<sdf version="1.6">
  <% 
    nb
    base_name
    north_array
    east_array
    color_array
  %>

  <world name="default">
    <gui>
      <camera name="user_camera">
        <pose>-5 0 1 0 0.2 0</pose>
      </camera>
    </gui>
    <physics type="ode">
      <ode>
        <solver>
          <type>quick</type>
          <iters>100</iters>
          <sor>1.0</sor>
        </solver>
        <constraints>
          <cfm>0.0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>0.1</contact_max_correcting_vel>
          <contact_surface_layer>0.0</contact_surface_layer>
        </constraints>
      </ode>
      <real_time_update_rate>-1</real_time_update_rate>
      <!--<max_step_size>0.0020</max_step_size>-->
    </physics>
    <gravity>0 0 -9.8</gravity>
    <include>
      <uri>model://sun</uri>
    </include>

    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>5000 5000</size>
            </plane>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>100</mu>
                <mu2>50</mu2>
              </ode>
            </friction>
          </surface>
        </collision>

        <visual name="ground">
        <pose>0 0 -0.1 0 0 -1.57</pose>
        <cast_shadows>false</cast_shadows>
        <geometry>
          <plane>
            <normal>0 0 1</normal>
            <size>80 80</size>
          </plane>
        </geometry>
        <material>
          <script>
            <uri>file:///home/ubuntu/ardupilot_gazebo/materials/scripts</uri>
            <uri>file:///home/ubuntu/ardupilot_gazebo/materials/textures</uri>
            <name>Burlington</name>
          </script>
        </material>
      </visual>

      </link>
    </model>
    <model name='Office Building'>
      <static>1</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <mesh>
              <uri>model://office_building/meshes/office_building.dae</uri>
            </mesh>
          </geometry>
          <max_contacts>10</max_contacts>
          <surface>
            <contact>
              <ode/>
            </contact>
            <bounce/>
            <friction>
              <torsional>
                <ode/>
              </torsional>
              <ode/>
            </friction>
          </surface>
        </collision>
        <visual name='visual'>
          <geometry>
            <mesh>
              <uri>model://office_building/meshes/office_building.dae</uri>
            </mesh>
          </geometry>
           <material>
            <script>
              <name>Gazebo/White </name>
              <uri>__default__</uri>
            </script>
        </material>
        </visual>
        <self_collide>0</self_collide>
        <enable_wind>0</enable_wind>
        <kinematic>0</kinematic>
      </link>
      <pose frame=''>-30 -31.5 0 0 0 -0.150001</pose>
    </model>

    <model name='Office Building_0'>
      <static>1</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <mesh>
              <uri>model://office_building/meshes/office_building.dae</uri>
            </mesh>
          </geometry>
          <max_contacts>10</max_contacts>
          <surface>
            <contact>
              <ode/>
            </contact>
            <bounce/>
            <friction>
              <torsional>
                <ode/>
              </torsional>
              <ode/>
            </friction>
          </surface>
        </collision>
        <visual name='visual'>
          <geometry>
            <mesh>
              <uri>model://office_building/meshes/office_building.dae</uri>
            </mesh>
          </geometry>
           <material>
            <script>
              <name>Gazebo/White </name>
              <uri>__default__</uri>
            </script>
        </material>
        </visual>
        <self_collide>0</self_collide>
        <enable_wind>0</enable_wind>
        <kinematic>0</kinematic>
      </link>
      <pose frame=''>-13 -34 0 0 0 -0.150001</pose>
    </model>
    <model name='Office Building_1'>
      <static>1</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <mesh>
              <uri>model://office_building/meshes/office_building.dae</uri>
            </mesh>
          </geometry>
          <max_contacts>10</max_contacts>
          <surface>
            <contact>
              <ode/>
            </contact>
            <bounce/>
            <friction>
              <torsional>
                <ode/>
              </torsional>
              <ode/>
            </friction>
          </surface>
        </collision>
        <visual name='visual'>
          <geometry>
            <mesh>
              <uri>model://office_building/meshes/office_building.dae</uri>
            </mesh>
          </geometry>
          <material>
            <script>
              <name>Gazebo/White </name>
              <uri>__default__</uri>
            </script>
        </material>
        </visual>
        <self_collide>0</self_collide>
        <enable_wind>0</enable_wind>
        <kinematic>0</kinematic>
      </link>
      <pose frame=''>4 -37 0 0 0 -0.150001</pose>
    </model>

    <model name='osrf_first_office'>
      <static>1</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <mesh>
              <uri>model://osrf_first_office/meshes/osrf_first_office.dae</uri>
            </mesh>
          </geometry>
          <max_contacts>10</max_contacts>
          <surface>
            <contact>
              <ode/>
            </contact>
            <bounce/>
            <friction>
              <torsional>
                <ode/>
              </torsional>
              <ode/>
            </friction>
          </surface>
        </collision>
        <visual name='visual'>
          <geometry>
            <mesh>
              <uri>model://osrf_first_office/meshes/osrf_first_office.dae</uri>
            </mesh>
          </geometry>
           <material>
            <script>
              <name>Gazebo/White </name>
              <uri>__default__</uri>
            </script>
        </material>
        </visual>
        <self_collide>0</self_collide>
        <enable_wind>0</enable_wind>
        <kinematic>0</kinematic>
      </link>
      <pose frame=''>45.0471 -14.389 -0.07 0 -0 1.3</pose>
    </model>


    <model name='Office Building'>
      <pose frame=''>-30 -31.5 0 0 0 -0.150001</pose>
      <scale>1 1 1</scale>
      <link name='link'>
        <pose frame=''>-30 -31.5 0 0 0 -0.150001</pose>
      </link>
    </model>

    <model name='Office Building_0'>
      <pose frame=''>-13 -34 0 0 0 -0.150001</pose>
      <scale>1 1 1</scale>
      <link name='link'>
        <pose frame=''>-13 -34 0 0 0 -0.150001</pose>
      </link>
    </model>

    <model name='Office Building_1'>
      <pose frame=''>4 -37 0 0 0 -0.150001</pose>
      <scale>1 1 1</scale>
      <link name='link'>
        <pose frame=''>4 -37 0 0 0 -0.150001</pose>
      </link>
    </model>

    <model name='osrf_first_office'>
      <pose frame=''>45.0471 -14.389 -0.07 0 -0 1.3</pose>
      <scale>1 1 1</scale>
      <link name='link'>
        <pose frame=''>45.0471 -14.389 -0.07 0 -0 1.3</pose>
      </link>
    </model>

    
    <% (0..nb.to_i-1).each do |i| %>
      <%
        east_floats = east_array.split(",").map {|s| s.to_f}
        north_floats = north_array.split(",").map {|s| s.to_f}
        colors = color_array.split(",")
        max_in_one_row = (nb.to_f**0.5).ceil
        namespace = base_name + '_' + i.to_s
        addr = '127.0.0.1'
        port_in = 9002 + i*10
        port_out = 9003 + i*10
        x = 0.0 + east_floats[i]
        y = 0.0 - north_floats[i]
        z = 0.194923
        roll = 0.0
        pitch = 0.0
        yaw = 0
        color = colors[i]
      %>
      
      <%= ERB.new(File.read(File.join('ardupilot_gazebo','models_gazebo','frog_v2_with_ardupilot','model.sdf.erb')), nil, nil, '_sub01').result(binding) %>
    <% end %>
  </world>
</sdf>
