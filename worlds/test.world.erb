              <torsional>
                <ode/>
              </torsional>
              <ode/>
            </friction>
          </surface>
        </collision>
        <visual name='visual'>
          <geometry>
            <mesh>
              <uri>model://osrf_first_office/meshes/osrf_first_office.dae</uri>
            </mesh>
          </geometry>
           <material>
            <script>
              <name>Gazebo/White </name>
              <uri>__default__</uri>
            </script>
        </material>
        </visual>
        <self_collide>0</self_collide>
        <enable_wind>0</enable_wind>
        <kinematic>0</kinematic>
      </link>
      <pose frame=''>45.0471 -14.389 -0.07 0 -0 1.3</pose>
    </model>


    <model name='Office Building'>
      <pose frame=''>-30 -31.5 0 0 0 -0.150001</pose>
      <scale>1 1 1</scale>
      <link name='link'>
        <pose frame=''>-30 -31.5 0 0 0 -0.150001</pose>
      </link>
    </model>

    <model name='Office Building_0'>
      <pose frame=''>-13 -34 0 0 0 -0.150001</pose>
      <scale>1 1 1</scale>
      <link name='link'>
        <pose frame=''>-13 -34 0 0 0 -0.150001</pose>
      </link>
    </model>

    <model name='Office Building_1'>
      <pose frame=''>4 -37 0 0 0 -0.150001</pose>
      <scale>1 1 1</scale>
      <link name='link'>
        <pose frame=''>4 -37 0 0 0 -0.150001</pose>
      </link>
    </model>

    <model name='osrf_first_office'>
      <pose frame=''>45.0471 -14.389 -0.07 0 -0 1.3</pose>
      <scale>1 1 1</scale>
      <link name='link'>
        <pose frame=''>45.0471 -14.389 -0.07 0 -0 1.3</pose>
      </link>
    </model>

    
    Random.new_seed
    <% (((-1*max_size...max_size.to_i).step(tree_spacing)).to_a - ((-1*30...30.to_i).step(tree_spacing)).to_a).each do |i| %>
      <% (-1*max_size...max_size.to_i).step(tree_spacing).each do |j| %>
        <%
          x = i+rand(5)
          y = j+rand(5) 
          z = 0.194923
          scaler = rand*3
          name=i.to_s+j.to_s
        %>

        <%= ERB.new(File.read(File.join('..','models_gazebo','custom_pine','model.sdf.erb')), nil, nil, '_sub01').result(binding) %>
        <% end %>
    <% end %>

    <% (((-1*max_size...max_size.to_i).step(tree_spacing)).to_a - ((-1*30...30.to_i).step(tree_spacing)).to_a).each do |j| %>
      <% (-1*30...30.to_i).step(5).each do |i| %>
        <%
          x = i+rand(5)
          y = j+rand(5) 
          z = 0.194923
          scaler = rand*3
          name=i.to_s+j.to_s
        %>

        <%= ERB.new(File.read(File.join('..','models_gazebo','custom_pine','model.sdf.erb')), nil, nil, '_sub01').result(binding) %>
        <% end %>
    <% end %>

  </world>
</sdf>
